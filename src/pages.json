		{
			"path": "pages/remark/remark",
			"style": {
				"navigationBarTitleText": "奈雪の茶",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/mine/member-code",
