{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue", // 匹配node_modules内@dcloudio的vue文件
			"cs-(.*)": "@/components/cs-$1/cs-$1.vue" // 匹配components目录内的vue文件
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"mp-alipay": {
					"transparentTitle": "always",
					"titlePenetrate": "YES"
				}
			}
		},
		{
			"path": "pages/menu/menu",
			"style": {
				"navigationBarTitleText": "菜单",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff",
				"mp-alipay":{
					"allowsBounceVertical": "NO" // 禁止页面拖动
				}
			}
		},
		{
			"path": "pages/take-foods/take-foods",
			"style": {
				"navigationBarTitleText": "点餐",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/mine/mine",
			"style": {
				"navigationBarTitleText": "奈雪乐园",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/invite/invite",
			"style": {
				"navigationBarTitleText": "买茶送包",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/packages/index",
			"style": {
				"navigationBarTitleText": "券包商品",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/packages/detail",
			"style": {
				"navigationBarTitleText": "购买劵包",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/address/address",
			"style": {
				"navigationBarTitleText": "我的地址",
				"navigationBarTextStyle": "black",
