				"navigationBarTitleText": "积分明细",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/balance/balance",
			"style": {
				"navigationBarTitleText": "余额储值",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/giftcard/giftcard",
			"style": {
				"navigationBarTitleText": "礼品卡列表",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff",
				"disableScroll": true
			}
		},
		{
			"path": "pages/attendance/attendance",
			"style": {
				"navigationStyle": "custom",
				"mp-alipay": {
					"transparentTitle": "always",
					"titlePenetrate": "YES"
				}
			}
		},
		{
			"path": "pages/services/services",
			"style": {
				"navigationBarTitleText": "更多服务",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/services/help-center",
			"style": {
				"navigationBarTitleText": "帮助中心",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录",
				"navigationBarTextStyle": "black",
				"navigationBarBackgroundColor": "#ffffff"
			}
		}
